{
    "presets": [
        "@babel/typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "transform-es2015-modules-commonjs",
        [
            "babel-plugin-transform-require-ignore",
            {
                "extensions": [".less", ".sass", ".scss"]
            }
        ]
    ]
}
